{
  "version": 2,
  "projects": {
    "dougs-mouvements-validation-server": {
      "root": "apps/dougs-mouvements-validation-server",
      "sourceRoot": "apps/dougs-mouvements-validation-server/src",
      "projectType": "application",
      "prefix": "dougs-mouvements-validation-server",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/dougs-mouvements-validation-server",
            "main": "apps/dougs-mouvements-validation-server/src/server.ts",
            "tsConfig": "apps/dougs-mouvements-validation-server/tsconfig.app.json",
            "assets": ["apps/dougs-mouvements-validation-server/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/dougs-mouvements-validation-server/src/environments/environment.ts",
                  "with": "apps/dougs-mouvements-validation-server/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "dougs-mouvements-validation-server:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/dougs-mouvements-validation-server/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/dougs-mouvements-validation-server"],
          "options": {
            "jestConfig": "apps/dougs-mouvements-validation-server/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "movements-validation-lib": {
      "root": "libs/movements-validation-lib",
      "sourceRoot": "libs/movements-validation-lib/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/movements-validation-lib/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/movements-validation-lib"],
          "options": {
            "jestConfig": "libs/movements-validation-lib/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "dougs-validation-movements-app": {
      "projectType": "application",
      "root": "apps/dougs-validation-movements-app",
      "sourceRoot": "apps/dougs-validation-movements-app/src",
      "prefix": "dougs-test",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/dougs-validation-movements-app",
            "index": "apps/dougs-validation-movements-app/src/index.html",
            "main": "apps/dougs-validation-movements-app/src/main.ts",
            "polyfills": "apps/dougs-validation-movements-app/src/polyfills.ts",
            "tsConfig": "apps/dougs-validation-movements-app/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/dougs-validation-movements-app/src/favicon.ico",
              "apps/dougs-validation-movements-app/src/assets"
            ],
            "styles": ["apps/dougs-validation-movements-app/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/dougs-validation-movements-app/src/environments/environment.ts",
                  "with": "apps/dougs-validation-movements-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "dougs-validation-movements-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "dougs-validation-movements-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": { "browserTarget": "dougs-validation-movements-app:build" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/dougs-validation-movements-app/src/**/*.ts",
              "apps/dougs-validation-movements-app/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/dougs-validation-movements-app"],
          "options": {
            "jestConfig": "apps/dougs-validation-movements-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": { "defaultCollection": "@nrwl/angular" },
  "defaultProject": "dougs-mouvements-validation-server",
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": { "style": "scss" }
  }
}
